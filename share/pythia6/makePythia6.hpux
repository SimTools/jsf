# /bin/sh
f77 -c  +Z +ppu pythia6131.f
f77 -c  +Z +ppu tpythia6_called_from_cc.F
f77 -c  +Z +ppu pythia6_common_block_address.F
echo 'void MAIN__() {}' >main.c
CC -c main.c
CC -c pythia6_address.c
ld -b -z -o libPythia6.so \
 main.o pythia6_address.o tpythia6_called_from_cc.o \
 pythia6_common_block_address.o pythia6131.o
chmod 555 libPythia6.so  
