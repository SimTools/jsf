#include "config/lclib.tmpl"
#include "conf/makejsf.tmpl"

#if __PYTHIA_VERSION__ >= 6
OBJS   = k0evt.o
LIB    = ../../src/readgenutil.o -L$(JSFROOT)/lib -lPythia6
FFLAGS = -I$(JSFROOT)/include 
#else
OBJS = k0evt.o
LIB  =  ../../src/readgenutil.o \
	-Wl,-L$(CERN_ROOT)/lib -ljetset74
FFLAGS = -I$(CERN_ROOT)/include
#endif

all:: k0evt

k0evt: $(OBJS)
	$(FC) $(OBJS) $(LIB) -o k0evt

